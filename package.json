  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "stylus --include-css static/css",
    "watch-css": "stylus -w --include-css static/css",
    "build-js": "browserify static/js/main.js | uglifyjs -mc > static/js/bundle.js",
    "watch-js": "watchify static/js/main.js -o static/js/bundle.js -dv",
    "build-abcd": "browserify static/abcd.js | uglifyjs -mc > static/abcd.bundle.js",
    "watch-abcd": "watchify static/abcd.js -o static/abcd.bundle.js -dv",
    "build": "npm run build-js && npm run build-css && npm run build-abcd",
    "watch": "npm run watch-js & npm run watch-css & npm run watch-abcd",
    "start": "node server.js"
  },
